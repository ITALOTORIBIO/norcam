                                        @Html.DisplayNameFor(model => model.fec_entrega)
                                    </dt>
                                    <dd class = "col-lg-6">
                                        @Html.DisplayFor(model => model.fec_entrega)
                                    </dd>
                                    </div>
                                    <div class="row">
                                    <dt class = "col-lg-6" style="text-align: right;">
                                        @Html.DisplayNameFor(model => model.tc)
                                    </dt>
                                    <dd class = "col-lg-6">
                                        @Html.DisplayFor(model => model.tc)
                                    </dd>
                                    </div>
                            </div>

                        </div>

                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>                            
                        </div>
                        </div>
                    </div>
