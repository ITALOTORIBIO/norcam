 @model Ordenes
 <!-- Modal -->
                    <div class="modal fade" id="Detalle" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">

                    <div class="modal-dialog modal-dialog-scrollable" role="document">
                        <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="exampleModalLabel">Detalle de la Orden</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                            <span aria-hidden="true">&times;</span>
                            </button>
                        </div>

                        <div class="modal-body">
                            <div class="container h-100">
                                <dl>
                                    <div class="row">
                                    <dt class = "col-lg-6" style="text-align: right;">
                                        @Html.DisplayNameFor(model => model.fec_rec)
                                    </dt>
                                    <dd class = "col-lg-6">
                                        @Html.DisplayFor(model => model.fec_rec)
                                    </dd>
                                    </div>
                                    <div class="row">
                                    <dt class = "col-lg-6" style="text-align: right;">
                                        @Html.DisplayNameFor(model => model.cod_cliente)
                                    </dt>
                                    <dd class = "col-lg-6">
                                        @Html.DisplayFor(model => model.cod_cliente)
                                    </dd>
                                    </div>
                                    <div class="row">
                                    <dt class = "col-lg-6" style="text-align: right;">
                                        @Html.DisplayNameFor(model => model.razon_social)
                                    </dt>
                                    <dd class = "col-lg-6">
                                        @Html.DisplayFor(model => model.razon_social)
                                    </dd>
                                    </div>
                                    <div class="row">
                                    <dt class = "col-lg-6" style="text-align: right;">
                                        @Html.DisplayNameFor(model => model.proveedor)
                                    </dt>
                                    <dd class = "col-lg-6">
                                        @Html.DisplayFor(model => model.proveedor)
                                    </dd>
                                    </div>
                                    <div class="row">
                                    <dt class = "col-lg-6" style="text-align: right;">
                                        @Html.DisplayNameFor(model => model.dua)
                                    </dt>
                                    <dd class = "col-lg-6">
                                        @Html.DisplayFor(model => model.dua)
                                    </dd>
                                    </div>
                                    <div class="row">
                                    <dt class = "col-lg-6" style="text-align: right;">
                                        @Html.DisplayNameFor(model => model.cif)
                                    </dt>
                                    <dd class = "col-lg-6">
                                        @Html.DisplayFor(model => model.cif)
                                    </dd>
                                    </div>
                                    <div class="row">
                                    <dt class = "col-lg-6" style="text-align: right;">
                                        @Html.DisplayNameFor(model => model.contenido)
                                    </dt>
                                    <dd class = "col-lg-6">
                                        @Html.DisplayFor(model => model.contenido)
                                    </dd>
                                    </div>
                                    <div class="row">
                                    <dt class = "col-lg-6" style="text-align: right;">
                                        @Html.DisplayNameFor(model => model.peso)
                                    </dt>
                                    <dd class = "col-lg-6">
                                        @Html.DisplayFor(model => model.peso)
                                    </dd>
                                    </div>
                                    <div class="row">
                                    <dt class = "col-lg-6" style="text-align: right;">
                                        @Html.DisplayNameFor(model => model.ubicacion)
                                    </dt>
                                    <dd class = "col-lg-6">
                                        @Html.DisplayFor(model => model.ubicacion)
                                    </dd>
                                    </div>
                                    <div class="row">
                                    <dt class = "col-lg-6" style="text-align: right;">
                                        @Html.DisplayNameFor(model => model.regimen)
                                    </dt>
                                    <dd class = "col-lg-6">
                                        @Html.DisplayFor(model => model.regimen)
                                    </dd>
                                    </div>
                                    <div class="row">
                                    <dt class = "col-lg-6" style="text-align: right;">
                                        @Html.DisplayNameFor(model => model.fec_num)
